@article{huang_simultaneous_2009,
	title = {Simultaneous {Evaluation} of the {Magnitude} and {Breadth} of a {Left} and {Right} {Censored} {Multivariate} {Response}, with {Application} to {HIV} {Vaccine} {Development}},
