	pmid = {26702094},
	pages = {319ra206--319ra206},
	file = {Full Text PDF:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\PJCWIJM3\\Lynch et al. - 2015 - Virologic effects of broadly neutralizing antibody.pdf:application/pdf;Snapshot:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\FS4QKKTM\\319ra206.html:text/html}
}

