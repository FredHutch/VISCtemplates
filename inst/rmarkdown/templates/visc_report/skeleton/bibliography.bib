	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	author = {West, Anthony P. and Scharf, Louise and Horwitz, Joshua and Klein, Florian and Nussenzweig, Michel C. and Bjorkman, Pamela J.},
	month = jun,
