	keywords = {Alleles, Animals, Carrier Proteins, CD4-Positive T-Lymphocytes, CD8-Positive T-Lymphocytes, Haplorhini, Humans, Mucous Membrane, Simian immunodeficiency virus},
	pages = {81ra36}
}

