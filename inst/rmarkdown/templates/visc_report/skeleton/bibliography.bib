	number = {3},
	urldate = {2015-10-18},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Stranford, Sharon A. and Skurnick, Joan and Louria, Donald and Osmond, Dennis and Chang, Sheng-Yung and Sninsky, John and Ferrari, Guido and Weinhold, Kent and Lindquist, Craig and Levy, Jay A.},
