@article{sharon_alternative_2003,
	title = {Alternative {Conformations} of {HIV}-1 {V}3 {Loops} {Mimic} β {Hairpins} in {Chemokines}, {Suggesting} a {Mechanism} for {Coreceptor} {Selectivity}},
