	keywords = {C4 domain, CD4bs, Clade C, Envelope, HIV-1, Neutralizing antibody, Recent infection, V3 loop},
	pages = {123--132},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\TNF9A9Z7\\Ringe et al. - 2011 - A single amino acid substitution in the C4 region .pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\3XXSD7DU\\S0042682211003230.html:text/html}
