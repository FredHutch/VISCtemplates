	pages = {6626--6633},
	file = {Full Text PDF:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\T2CVVWPP\\Parrish et al. - 2013 - Phenotypic properties of transmitted founder HIV-1.pdf:application/pdf;Snapshot:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\IQKEAQNC\\6626.html:text/html}
