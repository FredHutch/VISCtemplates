	pmid = {10708443},
	pages = {3264--3272},
	file = {Full Text PDF:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\5KUM9N6T\\Nguyen and Hildreth - 2000 - Evidence for Budding of Human Immunodeficiency Vir.pdf:application/pdf;Snapshot:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\ATN8M38N\\3264.html:text/html}
