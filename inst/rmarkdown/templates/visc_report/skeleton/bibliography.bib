	file = {PubMed Central Full Text PDF:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\6WW87C9C\\Shuler et al. - 2012 - Sports Health Benefits of Vitamin D.pdf:application/pdf}
