	pages = {666--677},
	file = {Full Text PDF:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\ZI34BUQG\\Gilbert et al. - 2005 - Correlation between Immunologic Responses to a Rec.pdf:application/pdf;Snapshot:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\ZQ8C59WK\\666.html:text/html}
