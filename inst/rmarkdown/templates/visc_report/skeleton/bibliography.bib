	file = {PLoS Full Text PDF:C\:\\Users\\adecamp\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\tey9iaiz.default\\zotero\\storage\\RKC99UJD\\Madsen and Browning - 2009 - A Groupwise Association Test for Rare Mutations Us.pdf:application/pdf}
}

@article{rakoff-nahoum_detection_2006,
